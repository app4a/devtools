  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next build",
    "lint": "next lint",
    "test": "jest",
    "test:clearCache": "jest --clearCache",
    "analyze": "ANALYZE=true npm run build",
    "seo:validate": "echo 'Validating SEO implementation...' && node scripts/validate-seo.js",
    "seo:sitemap": "echo 'Generating sitemap...' && node scripts/generate-sitemap.js",
    "submit-urls": "node scripts/auto-submit-urls.js",
    "submit-google": "node scripts/google-indexing-api.js",
    "submit-bing": "node scripts/bing-webmaster-api.js",
    "ping-sitemap": "node scripts/sitemap-ping.js",
    "seo-submit": "npm run ping-sitemap && npm run submit-urls",
    "post-deploy": "npm run seo-submit",
    "lighthouse": "lighthouse --output=html --output-path=./lighthouse-report.html --chrome-flags='--headless' http://localhost:3000",
    "build:production": "NODE_ENV=production npm run build",
    "deploy:github": "npm run build && touch out/.nojekyll",
    "preview": "npm run build && npx serve out",
    "test:github-build": "node scripts/test-github-build.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
